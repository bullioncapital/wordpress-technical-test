{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "create-block/kinesis-block",
  "version": "0.1.0",
  "title": "Kinesis Block",
  "category": "widgets",
  "icon": "smiley",
  "description": "Example block scaffolded with Create Block tool.",
  "example": {},
  "supports": {
    "html": false
  },
  "textdomain": "kinesis-block",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css",
  "attributes": {
    "title": {
      "type": "string",
      "source": "html",
      "selector": "h2"
    },
    "description": {
      "type": "string",
      "source": "html",
      "selector": "p"
    },
    "rows": {
      "type": "array",
      "default": []
    },
    "mainImage": {
      "type": "string",
      "source": "attribute",
      "attribute": "src",
      "selector": ".right-image img"
    },
    "link": {
      "type": "string",
      "source": "html",
      "selector": ".slink"
    }
  }
}